
import cvxpy as cp

# Define variables
num_days = 30
num_crops = 6

# Variables for planting decisions
x = cp.Variable((num_days, num_crops), integer = True)

# Variables for selling crop constraints
b = cp.Variable(num_days, nonneg= True)
constraints = []
#land
constraints.append(x[0,0] >= 10)
constraints.append(x[0,0]+x[0,1]+x[0,2]+2*x[0,3]+x[0,4]<=50)
constraints.append(x[0,0]+x[0,1]+x[0,2]+2*x[0,3]+x[0,4]+x[1,1]<=50)
constraints.append(x[0,0]+x[0,2]+x[1,1]+x[2,2]<=50)
constraints.append(x[0,0]+x[2,2]+x[3,1]<=50)
constraints.append(x[2,2]+x[3,1]+2*x[4,3]<=50)
constraints.append(2*x[4,3]+x[5,1]+x[5,2]<=50)
constraints.append(x[5,1]+x[5,2]+x[6,4]<=50)
constraints.append(x[5,2]+x[6,4]+x[7,1]<=50)
constraints.append(x[7,1]+x[8,2]<=50)
constraints.append(x[8,2]+x[9,0]+x[9,1]+2*x[9,3]<=50)
constraints.append(x[8,2]+x[9,0]+x[9,1]+2*x[9,3]<=50)
constraints.append(x[9,0] + x[11,1] + x[11,2] <= 50)
constraints.append(x[9,0] + x[11,1] + x[11,2] + x[12,5] <= 50)
constraints.append(x[11,2] + x[13,1] + x[13,4] + x[13,5] <= 50)
constraints.append(x[13,1] + x[13,4] + x[14,2] + 2*x[14,3] + x[14,5] <= 50)
constraints.append(x[14,2] + 2*x[14,3] +x[15,1] + x[15,5] <= 50)
constraints.append(x[14,2] + x[15,1] + x[16,5] <= 50)
constraints.append(x[17,1] + x[17,2] + x[17,5] <= 50)
constraints.append(x[17,1] + x[17,2] + x[18,5] <= 50)
constraints.append(x[17,2] + x[19,0] + x[19,1] + 2*x[19,3] + x[19,5] <= 75)
constraints.append(x[19,0] + x[19,1] + 2*x[19,3] + x[20,2] + x[20,4] + x[20,5] <= 75)
constraints.append(x[19,0] + x[20,2] + x[20,4] + x[21,1] + x[21,5] <= 75)
constraints.append(x[19,0] + x[20,2] + x[21,1] + x[22,5] <= 75)
constraints.append(x[23,1] + x[23,2] + x[23,5] <= 75)

constraints.append(b[10] - (0.5*x[11,1] + 2*x[11,2]) - 15*x[23,5] == b[11])
constraints.append(30*x[9,0] + 2*x[11,1] + b[11] - 15*x[24,5] == b[12])
constraints.append(8*x[11,2] + b[12] - (0.5*x[13,1] + 1.5*x[13,4]) - 15*x[25,5] == b[13])
constraints.append(2*x[13,1] + b[13] - (2*x[14,2] + 1.5*x[14,3]) - 15*x[26,5] == b[14])
constraints.append(6*x[13,4] + 8*x[14,3] + b[14] - 0.5*x[15,1] - 15*x[27,5] == b[15])
constraints.append(8*x[14,2] + 2*x[15,1] + b[15] - 15*x[28,5] == b[16])
constraints.append(b[16] - (0.5*x[17,1] + 2*x[17,2]) - 15*x[29,5] == b[17])
constraints.append(2*x[17,1] + b[17] == b[18])
constraints.append(8*x[17,2] + b[18] - (5*x[19,0] + 0.5*x[19,1] + 1.5*x[19,3]) - 50 == b[19])
constraints.append(2*x[19,1] + 8*x[19,3] + b[19] - (2*x[20,2] + 1.5*x[20,4]) == b[20])
constraints.append(b[20] - 0.5*x[21,1] == b[21])
constraints.append(30*x[19,0] + 8*x[20,2] + 6*x[20,4] + 2*x[21,1] + b[21] == b[22])
constraints.append(b[22] - (0.5*x[23,1] + 2*x[23,2]) == b[23])
constraints.append(2*x[23,1] + b[23] - 1.5*x[24,3] == b[24])
constraints.append(8*x[23,2] + b[24] - 0.5*x[25,1] == b[25])
constraints.append(8*x[24,3] + b[25] - 2*x[26,2] == b[26])
constraints.append(2*x[25,1] + b[26] - (0.5*x[27,1] + 1.5*x[27,4]) == b[27])
constraints.append(8*x[26,2] + 2*x[27,1] + b[27] == b[28])
constraints.append((6*x[27,4] + b[28]) * 1.5 == b[29])



#white carrot
constraints.append(x[12,5] + x[13,5] + x[14,5] + x[15,5] + x[16,5] + x[17,5] + x[18,5] + x[19,5] + x[20,5] + x[21,5] + x[22,5] + x[23,5] + x[24,5] + x[25,5] + x[26,5] + x[27,5] + x[28,5] + x[29,5] <= 2)

constraints.append(30*x[0,0] + b[2] - 0.5*x[3,1] - 15*x[15,5] == b[3])
constraints.append(2*x[3,1] + 8*x[2,2] + b[3] - 1.5*x[4,3] - 15*x[16,5] == b[4])

constraints.append(x[23,2] + 2*x[24,3] + x[25,1] + x[25,5] <= 75)
constraints.append(x[25,1] + x[26,2] + x[26,5] <= 75)
constraints.append(x[26,2] + x[27,1] + x[27,4] + x[27,5] <= 75)
constraints.append(x[26,2] + x[27,1] + x[27,4] + x[28,5] <= 75)
constraints.append(x[29,5] <= 75)

#Daily profit
constraints.append(20 -(5*(x[0,0] - 10) + 0.5*x[0,1]+2*x[0,2]+1.5*x[0,3]+1.5*x[0,4])-15*x[12,5] == b[0])
constraints.append(2*x[0,1] + 8*x[0,3] + b[0] - 0.5*x[1,1] - 15*x[13,5] == b[1])
